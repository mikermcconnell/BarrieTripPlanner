{
  "name": "barrie-transit-planner",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "npm run android:dev",
    "android:direct": "expo run:android",
    "android:recover": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/android-recover.ps1",
    "android:dev": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/android-dev.ps1",
    "android:dev:direct": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/android-dev.ps1 -Direct",
    "android:stable": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/android-stable.ps1",
    "android:stable:launch": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/android-stable.ps1 -SkipBuild",
    "android:stable:rebuild": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/android-stable.ps1",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "proxy": "node proxy-server.js",
    "web:dev": "start cmd /c \"node proxy-server.js\" && expo start --web",
    "fetch-addresses": "node scripts/fetchBarrieAddresses.js",
    "dev:android": "expo start --dev-client",
    "test": "jest",
    "test:watch": "jest --watch",
    "check:parity": "node scripts/check-platform-parity.js"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.8",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.10.1",
    "@sentry/react-native": "^7.12.1",
    "babel-preset-expo": "~54.0.10",
    "expo": "^54.0.32",
    "expo-asset": "~12.0.12",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-haptics": "~15.0.8",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.16",
    "expo-secure-store": "^15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "firebase": "^12.8.0",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0"
  },
  "private": true
}
